@model IEnumerable<SystemMartinezCV.Models.Roles>

@{
    ViewBag.Title = "Index";
}



<p>
    @Html.ActionLink("Agregar", "Create")
</p>
<table class="table jumbotron table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Rol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Rol)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdRol }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdRol }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdRol })
        </td>
    </tr>
}

</table>
